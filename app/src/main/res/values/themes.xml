<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AllOnOne" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#FBB8AC</item>
        <item name="colorPrimaryVariant">#442C2E</item>
        <item name="colorOnPrimary">#442C2E</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#FEEAE6</item>
        <item name="colorSecondaryVariant">#66B14664</item>
        <item name="colorOnSecondary">#442C2E</item>



        <item name="colorSurface">#FFFBFA</item>
        <item name="colorOnSurface">#442c2e</item>

        <item name="colorOnBackground">#442c2e</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.AllOnOne.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.AllOnOne.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.AllOnOne.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>